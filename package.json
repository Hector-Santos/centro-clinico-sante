{
  "name": "centro-clinico-sante",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/src/cloud.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "nest build",
    "clean": "rm -rf dist",
    "watch:dist": "tsc -w",
    "start:emulator": "firebase emulators:start --only functions",
    "emulator-init": "npm run clean && npm run build",
    "emulator": "concurrently -k \"npm:watch:dist\" \"npm run emulator-init && npm run start:emulator\"",
    "hard-clear": "rm -rf node_modules package-lock.json dist",
    "emulator-build": "rm -rf dist && npm run build && firebase emulators:start --only functions",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\" \"test/**/*.{ts,js,json,md}\" \"*.{ts,js,json,md}\"",
    "dev": "NODE_PATH=./ ts-node-dev --respawn src/local.ts",
    "start": "NODE_PATH=./ nodemon --watch src --ext ts --exec \"ts-node src/local.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/local.js",
    "deploy": "rm -rf dist && npm run build && firebase deploy --only functions",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0 --quiet --cache",
    "test:unit": "jest --testPathPattern=__tests__/unit --coverage",
    "test:integration": "jest --testPathPattern=__tests__/integration --coverage",
    "test:watch": "jest --watch",
    "test": "jest -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "log:preview": "ts-node-dev --respawn src/common/status-preview.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.2.7",
    "api": "^6.1.3",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.6.0",
    "kleur": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.12.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.7",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.4.19",
    "@types/express": "^4.17.17",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "firestore-jest-mock": "^0.21.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
